# QuakeWorld player name
set qw_name ""
# QuakeWorld server address
set qw_server ""
# QuakeWorld Server port
set qw_server_port 27500
# Rcon password (optional)
set qw_rcon_password ""
# Server password (optional)
set qw_password ""
# Encrypt rcon messages?
set qw_encrypt_rcon 1
# QuakeWorld transmission rate
set qw_rate 2500
# QuakeWorld message mode
set qw_msgmode 0
# QuakeWorld player bottomcolor
set qw_bottomcolor 0
# QuakeWorld player topcolor
set qw_topcolor 0

# IRC print colors
set qw_color_chattext 15;
set qw_color_statusmessage 9;
set qw_color_normaltext 16;
set qw_color_centerprint 6;

putlog "QuakeWorld IRC module TCL settings loaded."

